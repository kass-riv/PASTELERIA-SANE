<div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
  <div class="carousel-inner">
    <div class="carousel-item active">
      <img src="~/img/TecCarrusel1.jpg" class="d-block w-100" alt="..." style="width: 500px; height:250px">
    </div>
    <div class="carousel-item">
       <img src="~/img/TecCarrusel2.jpg" class="d-block w-100" alt="..." style="width: 500px; height:250px">
